        <script>
         function cleanHours(today) {
            if(today.toLowerCase() != 'closed'){
                var openHours = today.split('-'),
                openTime = parseInt(openHours[0].slice(0,1)),
                openTimeShow = openHours[0].replace(':00',''),
                closeTime = parseInt(openHours[1].slice(0,1)) + 12,
                closeTimeShow = openHours[0].replace(':00','') + '-' + openHours[1].replace(':00','');
                return closeTimeShow
            }
        }
        </script>

