        <script>
            function enterCheck(e) {
                if (13 == e.keyCode) {
                var target = event.target;
                target.click();
                }
            }

            function hideSearchForm() {
                var poiEl = document.getElementById('poiEl');
                poiEl.classList.add("moreInfo");
            }

            function hideSearchFormMobile() {
                var poiEl = document.getElementById('poiEl');
                var mapEl = document.getElementById('mapEl');
                poiEl.classList.add("moreInfo");
                var wrapperEl = document.getElementById('wrapper');
                wrapperEl.classList.add("wrapperCustom");
                mapEl.classList.remove('mobileInactive');
                mapEl.style.order="1";
            }

            function revealDummyDiv() {
                var locationsDiv = document.getElementById('locationsDiv');
                var dummyDiv = document.getElementById('dummyDiv');
                var dummyText = document.getElementById('dummyText');
                dummyText.textContent = locationsDiv.textContent;
                dummyDiv.classList.remove('mobileHidden');
                locationsDiv.classList.add('mobileHidden');
            }

            function hideDummyDiv() {
                var locationsDiv = document.getElementById('locationsDiv');
                var dummyDiv = document.getElementById('dummyDiv');
                dummyDiv.classList.add('mobileHidden');
                locationsDiv.classList.remove('mobileHidden');
            }

            function showMap() {
                revealDummyDiv();
                var mapEl = document.getElementById('mapEl');
                mapEl.classList.remove('mobileInactive');

                var poiEl = document.getElementById('poiEl');
                poiEl.classList.add('poiAdjust');

                var hamburgerBlack = document.getElementById('hamburgerBlack');
                var hamburgerGrey = document.getElementById('hamburgerGrey');
                var pinGrey = document.getElementById('pinGrey');
                var pinBlack = document.getElementById('pinBlack');

                hamburgerBlack.classList.add('mobileHidden');
                hamburgerGrey.classList.remove('mobileHidden');
                pinGrey.classList.add('mobileHidden');
                pinBlack.classList.remove('mobileHidden');

                var wrapperEl = document.getElementById('wrapper');
                wrapperEl.classList.add('adjustTop');
                
                var w2giHeight =  $('#w2gi_wrapper').height();
                console.log(w2giHeight);
                var newHeight = w2giHeight + 300;
                console.log(newHeight);
                var numChange = parseInt(newHeight);
                console.log(numChange);

                setTimeout(function(){
                    parent.postMessage("w2gi:iframeHeightUpdated" + "//" + numChange + "//" + $('#w2gi_wrapper').width(),"*");
                 }, 100);


            }

            function hideMap() {
                hideDummyDiv();
                var mapEl = document.getElementById('mapEl');
                mapEl.classList.add('mobileInactive');

                var poiEl = document.getElementById('poiEl');
                poiEl.classList.remove('poiAdjust');

                var hamburgerBlack = document.getElementById('hamburgerBlack');
                var hamburgerGrey = document.getElementById('hamburgerGrey');
                var pinGrey = document.getElementById('pinGrey');
                var pinBlack = document.getElementById('pinBlack');

                hamburgerBlack.classList.remove('mobileHidden');
                hamburgerGrey.classList.add('mobileHidden');
                pinGrey.classList.remove('mobileHidden');
                pinBlack.classList.add('mobileHidden');

                var wrapperEl = document.getElementById('wrapper');
                wrapperEl.classList.remove('adjustTop');
                parent.postMessage("w2gi:iframeHeightUpdated" + "//" + $('#w2gi_wrapper').height() + "//" + $('#w2gi_wrapper').width(),"*");
            }
        </script>
