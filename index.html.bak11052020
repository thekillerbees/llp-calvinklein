                 },
                 custom_ob: {
                   updateRecords : function(recs) {
                        var date = new Date();
                        var today = date.getDay();
                        var records;
                        $(recs).each (function(i,e) {
                            e.mon_hours_2 = cleanHours(e.mon_hours);
                            e.tues_hours_2 = cleanHours(e.tues_hours);
                            e.wed_hours_2 = cleanHours(e.wed_hours);
                            e.thurs_hours_2 = cleanHours(e.thurs_hours);
                            e.fri_hours_2 = cleanHours(e.fri_hours);
                            e.sat_hours_2 = cleanHours(e.sat_hours);
                            e.sun_hours_2 = cleanHours(e.sun_hours);

                            if(e.postalcode)
                            var postal = e.postalcode.length;

                            if(postal == 10) {
                                var cut = e.postalcode.substring(0, 5);
                                e.postalcode = cut;
                            }
                            //if(e.holidayhours){
                             //   temp = e.holidayhours;
                            //    e.holidayhours1=temp.replace(/,/g,"<br>");
                           // } 

                           if(e.holidayhours){
                                e.holidayhour = e.holidayhours.replace(/,/g,'<br>');
                            }

                            if (today == 0) {
                                e.SUNCHECK = 1;
                                e.TODAYHOURS = e.sun_hours_2;
                                e.OPENTODAY = e.sun_hours_2;
                            }

                            if (today == 1) {
                                e.MONCHECK = 1;
                                 e.TODAYHOURS = e.mon_hours_2;
                                 e.OPENTODAY = e.mon_hours_2;
                            }

                            if (today == 2) {
                                e.TUECHECK = 1;
                                e.TODAYHOURS = e.tues_hours_2;
                                e.OPENTODAY = e.tues_hours_2;
                            }

                            if (today == 3) {
                                e.WEDCHECK = 1;
                                e.TODAYHOURS = e.wed_hours_2;
                                e.OPENTODAY = e.wed_hours_2;
                            }

                            if (today == 4) {
                                e.THRCHECK = 1;
                                e.TODAYHOURS = e.thurs_hours_2;
                                e.OPENTODAY = e.thurs_hours_2;
                            }

                            if (today == 5) {
                                e.FRICHECK = 1;
                                e.TODAYHOURS = e.fri_hours_2;
                                e.OPENTODAY = e.fri_hours_2;
                            }

                            if (today == 6) {
                                e.SATCHECK = 1;
                                e.TODAYHOURS = e.sat_hours_2;
                                e.OPENTODAY = e.sat_hours_2;
                            }

                            if(e.city){
                              e.city2 = e.city.toLowerCase().replace(/ /g,"-");
                            }

                            if(e.state){
                              e.state2 = e.state.toLowerCase().replace(/ /g,"-");
                            }else if (e.province) {
                                e.state2 = e.province.toLowerCase().replace(/ /g,"-");
                            }

                        }.bind(this));
                        recs = shuffleArray(recs);
                        return recs;
                    },
                },
            }
