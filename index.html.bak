                                    <div id="filter_bar">
                                        <div>Filters</div>
                                    </div>
                                    <div class="filter_div">
                                         <img class="filter_unselected" src="images/retail.png" onclick="toggleFilter(event);" alt="First filter unselected">
                                         <img class="filter_selected" src="images/retail_highlighted.png" onclick="untoggleFilter(event);" alt="First filter selected">
                                         <span>Retail</span>
                                         <img class="check_image" src="images/filter_check.png" alt="First filter checkmark">
