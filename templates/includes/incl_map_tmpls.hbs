            <!-- POI table template start -->
            <table id="collection_poi" odd_row_class="odd" even_row_class="even" active_row_class="active" slice_size="5">
                <tbody >
                    <tr recnum="#{POINUM}">
                        <td>
                            <a action="open_bubble" recnum="#{POINUM}" href="javascript:void(0);" class='store_name' >#{NAME}</a><br/>
                            #{ADDRESS1}<br/>
                            #{CITY}, #{STATE}#{PROVINCE} #{POSTALCODE}<br />
                            Phone: #{PHONE}<br />
                            <a action="more_info" linktrack="POI - more info" recnum="#{POINUM}" href="javascript:void(0);" onClick="_gaq.push(['_trackEvent', 'Interactions', 'More Info']);">More Info</a><br />
                            Send Address To:
                            <a href="javascript: void(0);" action="linktrack" linktrack="POI - #{UID} - Send to Email" onclick="window.open('/lite?appkey={{{appkey}}}&action=emaillocatorstart&uid=#{UID}','EMAIL','width=680,height=450,status=yes,toolbar=no,menubar=no,location=no'); _gaq.push(['_trackEvent', 'Interactions', 'Send Location to Email']);">E-mail</a>
                            |
                            <a href="javascript: void(0);" action="linktrack" linktrack="POI - #{UID} - Send to Mobile" onclick="window.open('/lite?appkey={{{appkey}}}&action=smslocatorstart&uid=#{UID}','MOBILE','width=680,height=450,status=yes,toolbar=no,menubar=no,location=no'); _gaq.push(['_trackEvent', 'Interactions', 'Send Location to Phone']);">Mobile</a>

                        </td>
                    </tr>
                </tbody>
            </table>
            <!-- POI table template end -->

            <!-- More Info table template start -->
            <table id="collection_more_info" odd_row_class="odd" even_row_class="even" active_row_class="active" slice_size="5">
                <thead>
                    <tr><td></td></tr>
                </thead>
                <tfoot>
                    <tr><td></td></tr>
                </tfoot>
                <tbody>
                    <tr recnum="#{POINUM}">
                        <td width="33px" valign="top">
                            <div templateid="#{ICON}">
                            </div>
                        </td>
                        <td width="310px">
                            <a action="open_bubble" class="store_name" recnum="#{POINUM}" href="javascript:void(0);">#{NAME}</a><br/>
