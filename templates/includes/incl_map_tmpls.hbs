                                    <div class="address_line1" ></div>
                                    <div>
                                        <span class="address_city"></span>
                                        <span class="address_state"></span>
                                        <span class="address_zip"></span>
                                    </div>
                                </span>
                            </div>
                        </th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th class="maneuver">
                            <div>
